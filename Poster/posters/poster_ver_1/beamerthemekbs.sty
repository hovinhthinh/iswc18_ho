\mode<presentation>

\usetheme{infosys}

\setbeamercolor{bibliography entry author}{fg=KbsDarkBlue}
\setbeamercolor{bibliography entry title}{fg=black}
\setbeamercolor{bibliography entry journal}{fg=black}
\setbeamercolor{bibliography entry note}{fg=black}

\usecolortheme{default}
\setbeamercolor{structure}{fg=KbsDarkBlue}

% play around with block colors
\setbeamercolor{block body}{fg=black}
\setbeamercolor{block title}{fg=white,bg=KbsDarkBlue}
\setbeamercolor{example text}{fg=KbsDarkBlue}

% and we are done
\mode<all>


